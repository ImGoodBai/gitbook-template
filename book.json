{
    "title": "好宝宝起名网",
    "description": "分享中国传统文化知识，让每一个宝宝都能有满意的名字。",
    "author": "goodbai",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    "styles": {
        "website": "assets/styles/website.css"
    },
    "structure": {
        "readme": "README.md"
    },
    "plugins": [
        "-sharing",
        "sharing-plus@^0.0.2",
        "-highlight",
        "prism@^2.1.0",
        "prism-themes@^0.0.2",
        "-lunr",
        "advanced-emoji@^0.2.1",
        "expandable-chapters-small@^0.1.7",
        "tbfed-pagefooter@^0.0.1",
        "terminal@^0.3.2",
        "code@^0.1.0",
        "splitter@^0.0.8",
        "favicon@^0.0.2",
        "github-tables@^0.0.2",
        "image-captions@^3.1.0",
        "canonical-link@^2.0.3",
        "auto-scroll-table@^0.0.5",
        "logo@^0.0.2",
        "intopic-toc@^1.1.1",
        "meta@^0.1.12",
        "3-ba@^0.9.0",
        "ga4@^1.0.1",
        "lanying-grow-ai@^1.0.5",
        "lanying-grow-ai-theme@^1.0.8",
        "lanying-grow-ai-expandable-chapters@^0.1.8",
        "lanying-grow-ai-image-captions@^3.1.1"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": false
        },
        "prism": {
            "css": [
                "prism-themes/themes/prism-base16-ateliersulphurpool.light.css"
            ]
        },
        "github-buttons": {
            "repo": "ImGoodBai/qiming",
            "types": [
                "star"
            ],
            "size": "small"
        },
        "include-codeblock": {
            "template": "ace",
            "unindent": true,
            "edit": true
        },
        "tbfed-pagefooter": {
            "copyright": "@古德白 | <a href='https://qiming.suishouai.cn/' style='text-decoration:none!important;'>官网</a> | <a href='/sitemap.xml' style='text-decoration:none!important;' target='_blank'>网站地图</a>",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        },
        "edit-link": {
            "base": "https://github.com/ImGoodBai/qiming/blob/master",
            "label": ""
        },
        "favicon": {
            "shortcut": "/assets/favicon.ico",
            "bookmark": "/assets/favicon.ico"
        },
        "terminal": {
            "copyButtons": true,
            "fade": false,
            "style": "flat"
        },
        "sharing": {
            "douban": true,
            "facebook": false,
            "google": false,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": false,
            "linkedin": false,
            "messenger": false,
            "pocket": false,
            "qq": true,
            "qzone": false,
            "stumbleupon": false,
            "twitter": false,
            "viber": false,
            "vk": false,
            "weibo": true,
            "whatsapp": false,
            "all": [
                "facebook",
                "google",
                "twitter",
                "weibo",
                "instapaper",
                "linkedin",
                "pocket",
                "stumbleupon",
                "qq",
                "qzone",
                "douban",
                "line",
                "whatsapp"
            ]
        },
        "logo": {
            "logo": "/assets/logo.png",
            "logoStyle": "width: 100%;align: left;",
            "url": "https://qiming.suishouai.cn/"
        },
        "intopic-toc": {
            "selector": ".markdown-section h1, .markdown-section h2, .markdown-section h3",
            "mode": "nested",
            "maxDepth": 3,
            "isCollapsed": false,
            "isScrollspyActive": true,
            "visible": true,
            "label": "内容"
        },
        "language-picker": {
            "grid-columns": 3
        },
        "canonical-link": {
            "baseURL": "CanonicalLink"
        },
        "image-captions": {
            "caption": "_CAPTION_"
        },
        "meta": {
            "data": [
                {
                    "name": "keywords",
                    "content": "好宝宝起名网，宝宝起名，古诗词起名，龙宝宝起名，双胞胎起名"
                }
            ]
        },
        "3-ba": {
            "token": "c0091c1bb682847c68053df27bbc5cc4"
        },
        "ga4": {
            "tag": ""
        },
        "lanying-grow-ai": {
            "footer_note": "好宝宝起名网  1对1宝宝起名服务",
            "lanying_link": "",
            "sitemap_hostname": "CanonicalLink"
        }
    }
}
